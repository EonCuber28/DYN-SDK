  },
  "brackets": [
    ["(", ")"],
    ["{", "}"],
    ["[", "]"]
  ],
  "autoClosingPairs": [
    {"open": "(", "close": ")"},
    {"open": "{", "close": "}"},
    {"open": "[", "close": "]"},
    {"open": "\"", "close": "\""},
    {"open": "'''", "close": "'''"}
  ],
  "surroundingPairs": [
    ["(", ")"],
    ["{", "}"],
    ["[", "]"],
    ["\"", "\""],
    ["'''", "'''"]
  ],
  "folding": {
    "markers": {
