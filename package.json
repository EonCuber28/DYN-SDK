{
  "name": "dyn-language",
  "displayName": "DYN Language Support",
  "description": "Syntax highlighting and language support for DYN FTC autonomous scripts",
  "version": "0.0.1",
  "publisher": "EonCuber28",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "dyn",
    "ftc",
    "robotics",
    "autonomous"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EonCuber28/dyn-language"
  },
  "bugs": {
    "url": "https://github.com/EonCuber28/dyn-language/issues"
  },
  "homepage": "https://github.com/EonCuber28/dyn-language#readme",
  "icon": "icon.png",
  "contributes": {
    "languages": [
      {
        "id": "dyn",
        "aliases": [
          "DYN",
          "dyn"
        ],
        "extensions": [
          ".dyn"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dyn",
        "scopeName": "source.dyn",
        "path": "./syntaxes/dyn.tmLanguage.json"
      }
    ]
  }
}